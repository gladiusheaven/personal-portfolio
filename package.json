{
  "name": "react-engineer-portfolio-fullstack",
  "version": "1.0.0",
  "description": "Full-stack portfolio with React frontend and Node.js backend. The frontend is served as static files, and the backend handles API requests.",
  "main": "server/dist/server.js",
  "scripts": {
    "build": "tsc -p server/tsconfig.json",
    "start": "node server/dist/server.js",
    "dev": "nodemon --watch 'server/**/*.ts' --exec 'ts-node' server/server.ts",
    "serve": "npm run build && npm start"
  },
  "dependencies": {
    "@google/genai": "^1.29.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.12",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}